{
    "name": "kafkiansky/symfony-middleware",
    "description": "PSR-15 Middleware for symfony.",
    "type": "library",
    "keywords": [
        "symfony",
        "psr-15",
        "middleware"
    ],
    "autoload": {
        "psr-4": {
            "Kafkiansky\\SymfonyMiddleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kafkiansky\\SymfonyMiddleware\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "symfony/dependency-injection": "^6.2",
        "symfony/event-dispatcher": "^6.2",
        "symfony/http-kernel": "^6.2",
        "symfony/config": "^6.2",
        "symfony/http-foundation": "^6.2",
        "psr/http-server-middleware": "^1.0",
        "psr/http-server-handler": "^1.0",
        "symfony/psr-http-message-bridge": "^2.1",
        "nyholm/psr7": "^1.4",
        "symfony/cache": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "v.zanfir",
            "email": "vadimzanfir@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
        "lint": "./vendor/bin/psalm --no-cache",
        "test": "./vendor/bin/phpunit --do-not-cache-result"
    }
}
